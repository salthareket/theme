//sidebar
$sidebar-width   : 360px;
$sidebar-width-offcanvas : 380px;
$sidebar-padding : 20px 25px;
$sidebar-border-radius:8px;
$sidebar-bg:#fff;
$sidebar-mobile-margin-top:100px;
$sidebar-item-gap:20px;
//common
$sidebar-font-family  :$font-primary;
$sidebar-font-size    :14px;
$sidebar-font-weight  :400;
$sidebar-color        :#333;
$sidebar-color-active :$base-color;
$sidebar-color-hover  :#000;
$sidebar-item-padding :12px 15px;
//child
$sidebar-child-font-family  :$font-primary;
$sidebar-child-font-size    :14px;
$sidebar-child-color        :#666;
$sidebar-child-color-active :$base-color;
$sidebar-child-color-hover  :#333;
$sidebar-child-indent       :18px;






/***************************************

B R E A D C R U M B

***************************************/
$breadcrumb-item-color:#777;
$breadcrumb-item-color-hover:#222;
$breadcrumb-item-font-size: 14px;
$breadcrumb-item-font-weight:400;
$breadcrumb-item-line-height:100%;
$breadcrumb-sep-color:$base-color;



/***************************************

P A G I N A T I O N

***************************************/
$pagination-font-family:$font-primary;
$pagination-font-size:14px;
$pagination-font-weight:500;
$pagination-font-weight-active:600;
$pagination-item-color:#666;
$pagination-item-color-active:#fff;
$pagination-item-color-hover:#333;
$pagination-item-bg-color:#fff;
$pagination-item-bg-color-hover:#fff;
$pagination-item-bg-color-active:$base-color;
$pagination-item-border:1px solid #ddd;
$pagination-item-border-hover:1px solid #ccc;
$pagination-item-border-active:1px solid $base-color;
$pagination-item-border-radius:4px;
$pagination-item-gap:10px;
@if strip-unit($pagination-item-gap) == 0 and strip-unit($pagination-item-border-radius) > 0 {
    $pagination-item-gap:20px;
}
$pagination-align:center;

$pagination-nav-font-family:$icon-font;
$pagination-nav-color:#9f9f9f;
$pagination-nav-color-hover:#333;
$pagination-nav-color-disabled:lighten(#9f9f9f,25%);
$pagination-nav-bg-color:transparent;
$pagination-nav-bg-color-hover:transparent;
$pagination-nav-border:1px solid transparent;
$pagination-nav-border-hover:1px solid transparent;
$pagination-nav-border-active:1px solid transparent;
$pagination-nav-border-radius:0px;
$pagination-nav-font-size:16px;
$pagination-nav-prev-text:'\f053';
$pagination-nav-next-text:'\f105';





/***************************************

F O R M S

***************************************/

$form-input-color : #666;
$form-input-color-focus : #333;
$form-input-font : $font-primary;
$form-input-font-weight : 500;
$form-input-text-transform:normal;
$form-input-bg-color : #fff;
$form-input-bg-color-focus : #fff;
$form-input-border-top : 1px solid rgba(#ccc, 1);
$form-input-border-bottom : 1px solid rgba(#ccc, 1);
$form-input-border-left : 1px solid rgba(#ccc, 1);
$form-input-border-right : 1px solid rgba(#ccc, 1);
$form-input-border-radius : 8px;

$form-input-border-focus-top : 1px solid rgba(#aaa, 1);
$form-input-border-focus-bottom : 1px solid rgba(#aaa, 1);
$form-input-border-focus-left : 1px solid rgba(#aaa, 1);
$form-input-border-focus-right : 1px solid rgba(#aaa, 1);

$form-input-color-placeholder : rgba(#a3a3a3, 1);
$form-input-font-weight-placeholder : 400;
$form-input-text-transform-placeholder:normal;

$form-option-color : #fff;
$form-option-color-active : #ebad19;
$form-option-border: 1px solid #aaa;

$form-label-font-size:18px;
$form-label-font-size-xl:18px;
$form-label-font-size-lg:16px;
$form-label-font-size-md:15px;
$form-label-font-size-sm:15px;
$form-label-color: #111;
$form-label-font-weight:600;

$form-spinner-button-color:#666;
$form-spinner-button-bg-color:#fff;
$form-spinner-button-color-hover:#fff;
$form-spinner-button-bg-color-hover:#ebad19;

//errors
$form-input-border-error : red;
$form-input-bg-color-error : rgba(#faeeee,.3);
$form-input-color-error : inherit;
$form-input-color-placeholder-error : lighten(red, .5);


/* page slider */
$slider-title-font-size:60px;
$slider-title-font-line-height:$slider-title-font-size;

/* Plugin Versions*/
$bootstrap-select-version : 1.13.7;

$body-bg-color : #fff;//#eae9e4;//#e2e3e4;//#fcfcfc;
$body-bg-backdrop: #293a91;

/* Fonts */
$font-primary: "arial";//'Quicksand', sans-serif;
$header-font: "arial";//'Quicksand', sans-serif;
$nav-font:  "arial";
$icon-font: 'Font Awesome 6 Pro';
$icon-font-brands: 'Font Awesome 6 Brands';
$use-fontawesome: true;

$base-font-size : 15px;
$base-font-size-rem : #{$base-font-size / 16px * 1rem};
$base-font-line-height : 1.6;
$base-font-weight : 500;
$base-font-color : #333;


/* Colors */
$base-color: #382e2c;
$base-color-dark : #382e2c;
$base-color-light : #bf9f70;

$base-colors : (
    blue : (
        base: #0064af,
        dark: darken(#0064af, 15%),
        light:lighten(#0064af, 60%)
    ),
    blue-light : (
        base: #217fc8,
        dark: darken(#217fc8, 15%),
        light:lighten(#217fc8, 60%)
    )
);
$base-color:#0064af;


/* typography */
$typography_h1_font:$header-font;
$typography_h1_size:32px;
$typography_h1_size_sm:28px;
$typography_h1_weight:800;

$typography_h2_font:$header-font;
$typography_h2_size:28px;
$typography_h2_size_sm:24px;
$typography_h2_weight:700;

$typography_h3_font:$header-font;
$typography_h3_size:24px;
$typography_h3_size_sm:20px;
$typography_h3_weight:600;

$typography_h4_font:$header-font;
$typography_h4_size:20px;
$typography_h4_size_sm:18px;
$typography_h4_weight:600;

$typography_h5_font:$header-font;
$typography_h5_size:18px;
$typography_h5_size_sm:16px;
$typography_h5_weight:600;

$typography_h6_font:$header-font;
$typography_h6_size:16px;
$typography_h6_size_sm:16px;
$typography_h6_weight:600;



                                    {{function('inline_svg', logo_mobile, {"class": "logo-svg logo-mobile w-auto"})}}
                                {% else %}
                                    <img src="{{logo_mobile}}" class="logo logo-mobile" alt="{{site.name}}" fetchpriority="high" decoding="sync" />
                                {% endif %}
                            {% endif %}
                        </a>
                    </div>
                </div>
            {% elseif header_options.center.type == "navigation" %}

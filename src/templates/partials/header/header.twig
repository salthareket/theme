<div class="navbar-main navbar">           
    <div class="navbar-top {{header_options.container}} container px-4 px-lg-3 justify-content-between align-items-start">

        {% if header_options.start.type == "empty" %}
            <div class="{{header_options.start.class}}"></div>
        {% else %}
            {% if header_options.start.type == "brand" %}
                <div class="{{header_options.start.class}} {% if header_options.start.logo_height %}h-100{% else %}h-inherit{% endif %}">
                    <div class="navbar-brand align-items-{{header_options.start.align}} d-flex {% if logo_affix %}has-logo-affix{% endif %} {% if logo_mobile %}logo-mobile-{{logo_mobile_breakpoint}} logo-mobile{{logo_mobile_breakpoint}}{% endif %}">
                        <a href="{{site.url}}" class="mh-100 h-auto d-block btn-loading-page h-inherit" rel="home" itemprop="url" title="{{translate("Anasayfa")}}">
                            {% if function('get_extension', logo) == "svg" %}
                                {{function('inline_svg', logo, {"class": "logo-svg w-auto h-inherit--"})}}
                            {% else %}
                                <img src="{{logo}}" class="logo h-inherit--" alt="{{site.name}}" fetchpriority="high" decoding="sync" />
                            {% endif %}
                            {% if logo_affix != "" %}
                                {% if function('get_extension', logo_affix) == "svg" %}
                                    {{function('inline_svg', logo_affix, {"class": "logo-svg logo-affix w-auto h-inherit--"})}}
                                {% else %}
                                    <img src="{{logo_affix}}" class="logo logo-affix h-inherit--" alt="{{site.name}}"/>
                                {% endif %}
                            {% endif %}
                            {% if logo_mobile != "" %}
                                {% if function('get_extension', logo_mobile) == "svg" %}
                                    {{function('inline_svg', logo_mobile, {"class": "logo-svg logo-mobile w-auto"})}}
                                {% else %}
                                    <img src="{{logo_mobile}}" class="logo logo-mobile" alt="{{site.name}}" fetchpriority="high" decoding="sync" />
                                {% endif %}
                            {% endif %}
                        </a>
                    </div>
                </div>
            {% elseif header_options.start.type == "navigation" %}
                <div class="{{header_options.start.class}} h-100">
                    <nav id="navigation" class="navbar-container d-xl-flex d-none h-100 justify-content-{{header_options.start.align}}" role="navigation" aria-label="{{ translate("Navigasyon") }}">
                        <ul class="smartmenu navbar-nav-main nav navbar-nav--" data-sm-skip data-sm-skip-collapsible-behavior="true">
                            {% include "navigation/menu.twig" with {'menu': get_menu(header_options.start.menu).get_items, 'parent_link' : header_options.start.parent_link} %}
                        </ul>
                    </nav>
                </div>
            {% elseif header_options.start.type == "tools" %}
                {% include "partials/header/header-tools.twig" with { header_tools : header_options.start.tools, position: "start" } %}
            {% endif %}
        {% endif %}




        {% if header_options.center.type == "empty" %}
            <div class="{{header_options.center.class}}"></div>
        {% else %}
            {% if header_options.center.type == "brand" %}
                <div class="{{header_options.center.class}} {% if header_options.start.logo_height %}h-100{% else %}h-inherit{% endif %}">
